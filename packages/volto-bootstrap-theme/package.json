{
  "name": "@codesyntax/volto-bootstrap-theme",
  "version": "1.0.0-alpha.4",
  "description": "A new project using Plone 6.",
  "main": "src/index.js",
  "license": "MIT",
  "keywords": [
    "volto-addon",
    "volto",
    "plone",
    "react"
  ],
  "author": "Plone Foundation",
  "homepage": "https://github.com/collective/volto-bootstrap-theme#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/codesyntax/volto-bootstrap-theme.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "addons": [
    "@codesyntax/volto-bootstrap-theme",
    "@eeacms/volto-accordion-block",
    "@kitconcept/volto-slider-block",
    "@kitconcept/volto-button-block",
    "@eeacms/volto-columns-block",
    "@eeacms/volto-block-style",
    "@eeacms/volto-tabs-block"
  ],
  "theme": "@codesyntax/volto-bootstrap-theme",
  "scripts": {
    "i18n": "rm -rf build/messages && NODE_ENV=production i18n --addon",
    "dry-release": "release-it --dry-run",
